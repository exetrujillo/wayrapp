{
  "entryPoints": [
    "./src"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "name": "WayrApp Backend & Ecosystem Documentation",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeNotDocumented": true,
  "readme": "./README.md",
  "tsconfig": "./tsconfig.docs.json",
  "skipErrorChecking": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "plugin": [
    "typedoc-material-theme"
  ],
  "themeColor": "#50A8B1",
  "darkHighlightTheme": "dark-plus",
  "sort": [
    "source-order"
  ],
  "searchInComments": true,
  "cleanOutputDir": true,
  "highlightLanguages": [
    "typescript",
    "javascript",
    "json",
    "bash",
    "sh",
    "dockerfile",
    "yaml"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": false,
    "compactFolders": true,
    "includeFolders": false,
    "excludeReferences": true
  },
  "categorizeByGroup": false,
  "defaultCategory": "Other"
}